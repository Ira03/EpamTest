{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current", "browsers": ["last 2 versions", "ie >= 11" ]
      }, "corejs": 3, "useBuiltIns": "usage"}],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "env": {
    "test": { "presets": ["@babel/preset-env"] }
  },
  "plugins": ["@babel/plugin-transform-runtime"]
}
